<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.podcats">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StuffMewActivity"
            android:label="@string/smsk_title_name"/>

        <activity android:name=".ThisAmeowicanActivity"
            android:label="@string/this_ameowican_life_title_name"/>

        <activity android:name=".NowPlaying"
            android:label="@string/now_playing"/>
    </application>

</manifest>